{
  pkgs,
  lib,
  extraConfig,
  ...
}:
{
  programs.autorandr = {
    enable = true;
    profiles = {
      "computerone" = {
        fingerprint = {
          DP-0 = "";
          DP-2 = "";
        };
        config = {
          DP-2 = {
            enable = true;
            crtc = 1;
            mode = "1920x1080";
            position = "0x0";
            rate = "74.99";
            gamma = "1.19:1.0:0.833";
          };
          DP-0 = {
            mode = "2560x1440";
            position = "1920x0";
            rate = "169.83";
            crtc = 0;
            gamma = "1.19:1.0:0.833";
            primary = true;
          };
        };
      };
    };
  };

  systemd.user.services.autorandr = {
    Unit = {
      Description = "autorandr";
    };

    Install = {
      WantedBy = [ "autostart.target" ];
    };

    Service = {
      ExecStart = "${lib.getExe pkgs.autorandr} --load ${extraConfig.hostname}";
      Type = "oneshot";
    };
  };
}
